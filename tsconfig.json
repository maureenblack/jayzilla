{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "outDir": "./js",
        "rootDir": "./js",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["ES6", "DOM", "DOM.Iterable", "ESNext"]
    },
    "include": ["js/**/*"],
    "exclude": ["node_modules"]
}
